@{
    Layout = "/Views/Shared/_Layout.cshtml";

}

@model IEnumerable<Lab_04.Models.NoteStatus>
<!DOCTYPE html>
<html>
<head>
    <title>"Note Statuses"</title>
</head>
<style>
    table {
        border-collapse: collapse;
    }

    th, td {
        border: 1px solid black;
        padding: 8px;
    }
</style>
<body>
    <table cellpadding="5">
        <tr>
            <th>NoteStatusID</th>
            <th>Status Name</th>
        </tr>
        @foreach (var noteStatus in Model)
        {
            <tr>
                <td>@noteStatus.NoteStatusId</td>
                <td>@noteStatus.StatusName</td>
            </tr>
        }
    </table>
    <a href="https://localhost:7043/NoteStatuses/Index">NoteStatuses</a>
    <br />
    <a href="https://localhost:7043/NoteTags/Index">NoteTags</a>
    <br />
    <a href="https://localhost:7043/Users/Index">Users</a>
    <br />
    <a href="https://localhost:7043/SubNotes/Index">SubNotes</a>
    <br />
    <a href="https://localhost:7043/Notes/Index">Notes</a>
    <br />
</body>
</html>